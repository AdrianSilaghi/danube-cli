{
  "name": "@danubedata/cli",
  "version": "0.1.0",
  "description": "Deploy static sites to DanubeData from the terminal",
  "type": "module",
  "bin": {
    "danube": "./bin/danube"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin",
    "dist"
  ],
  "keywords": [
    "danubedata",
    "deploy",
    "static-site",
    "cli"
  ],
  "author": "DanubeData <adrian.silaghi@danubedata.ro>",
  "license": "MIT",
  "homepage": "https://danubedata.ro",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdrianSilaghi/danube-cli.git"
  },
  "bugs": {
    "url": "https://github.com/AdrianSilaghi/danube-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.1",
    "archiver": "^7.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "ignore": "^7.0.5",
    "ora": "^9.3.0"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
